  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.18.3",
  "devDependencies": {
    "@testcontainers/mongodb": "^11.11.0",
    "@testcontainers/postgresql": "^11.11.0",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.9",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "express": "^5.2.1",
    "mongodb": "^6.15.0",
    "pg": "^8.17.1",
    "supertest": "^7.2.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
